CC	= @CC@
CFLAGS	= @IRC_CFLAGS@
RM	= @RM@

#CFLAGS = -g -Wall

all: source
	@cd source ; $(MAKE) "CC=$(CC)" "CFLAGS=$(CFLAGS)"

build: all

clean:
	@cd source ; $(MAKE) clean

distclean:
	@cd source ; $(MAKE) distclean
	${RM} -f Makefile

# End of Makefile
